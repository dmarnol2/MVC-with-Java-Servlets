<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE web-app
    PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
    "http://java.sun.com/dtd/web-app_2_3.dtd">
<web-app>
  <display-name>Lab2</display-name>
  <description>User Pref, Convo State, and MVC</description>
  
  <servlet>
    <servlet-name>Welcome</servlet-name>
    <servlet-class>lab2_dmarnol2.WelcomeServlet</servlet-class>
    	<load-on-startup>1</load-on-startup>
  </servlet>

  <servlet-mapping>
    <servlet-name>Welcome</servlet-name>
    <url-pattern>/welcome</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>Controller</servlet-name>
    <servlet-class>lab2_dmarnol2.Controller</servlet-class>
    
    <load-on-startup>1</load-on-startup>
  </servlet>

  <servlet-mapping>
    <servlet-name>Controller</servlet-name>
    <url-pattern>/controller</url-pattern>
  </servlet-mapping>

   <servlet>
    <servlet-name>UserController</servlet-name>
    <servlet-class>lab2_dmarnol2.UserController</servlet-class>
      <load-on-startup>1</load-on-startup>
  </servlet>

  <servlet-mapping>
    <servlet-name>UserController</servlet-name>
    <url-pattern>/usercontroller</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>RemoveUser</servlet-name>
    <servlet-class>lab2_dmarnol2.RemoveUser</servlet-class>
      <load-on-startup>1</load-on-startup>
  </servlet>

  <servlet-mapping>
    <servlet-name>RemoveUser</servlet-name>
    <url-pattern>/removeuser</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>Login</servlet-name>
    <servlet-class>lab2_dmarnol2.LoginServlet</servlet-class>
    <init-param>
                <param-name>personfile</param-name>
                <param-value>C:\eclipse-workspace\lab2_dmarnol2\lab2.xml</param-value>
    </init-param>
    <load-on-startup>1</load-on-startup>
  </servlet>

  <servlet-mapping>
    <servlet-name>Login</servlet-name>
    <url-pattern>/</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>Display</servlet-name>
    <servlet-class>lab2_dmarnol2.DisplayInputs</servlet-class>
      <load-on-startup>1</load-on-startup>
  </servlet>

  <servlet-mapping>
    <servlet-name>Display</servlet-name>
    <url-pattern>/displayInputs</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>Data</servlet-name>
    <servlet-class>lab2_dmarnol2.ShowResults</servlet-class>
      <load-on-startup>1</load-on-startup>
  </servlet>

  <servlet-mapping>
    <servlet-name>Data</servlet-name>
    <url-pattern>/data</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>Names</servlet-name>
    <servlet-class>lab2_dmarnol2.EnterNames</servlet-class>
      <load-on-startup>1</load-on-startup>
  </servlet>

    <servlet-mapping>
    <servlet-name>Names</servlet-name>
    <url-pattern>/enterNames</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>Languages</servlet-name>
    <servlet-class>lab2_dmarnol2.EnterLanguages</servlet-class>
      <load-on-startup>1</load-on-startup>
  </servlet>

    <servlet-mapping>
    <servlet-name>Languages</servlet-name>
    <url-pattern>/enterLanguages</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>Days</servlet-name>
    <servlet-class>lab2_dmarnol2.EnterDays</servlet-class>
      <load-on-startup>1</load-on-startup>
  </servlet>

    <servlet-mapping>
    <servlet-name>Days</servlet-name>
    <url-pattern>/enterDays</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>Gender</servlet-name>
    <servlet-class>lab2_dmarnol2.EnterGender</servlet-class>
      <load-on-startup>1</load-on-startup>
  </servlet>

    <servlet-mapping>
    <servlet-name>Gender</servlet-name>
    <url-pattern>/enterGender</url-pattern>
  </servlet-mapping>

    






